                                    <div class="tab-pane fade" id="account-detail" role="tabpanel" aria-labelledby="account-detail-tab">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5>Add Address</h5>
                                            </div>
                                            <div class="card-body">
                                              
                                                <form action="/add_address" method="POST">
                                                    <div class="row">
                                                      <div class="form-group col-md-6">
                                                        <label>First Name <span class="required">*</span></label>
                                                        <input required="" class="form-control square" name="fname" type="text">
                                                      </div>
                                                      <div class="form-group col-md-6">
                                                        <label>Last Name <span class="required">*</span></label>
                                                        <input required="" class="form-control square" name="lname" type="text">
                                                      </div>
                                                      <div class="form-group col-md-6">
                                                        <label>Street <span class="required">*</span></label>
                                                        <input required="" class="form-control square" name="street" type="text">
                                                      </div>
                                                      <div class="form-group col-md-6">
                                                        <label>Apartment <span class="required">*</span></label>
                                                        <input required="" class="form-control square" name="apartment" type="text">
                                                      </div>
                                                      <div class="form-group col-md-6">
                                                        <label>City <span class="required">*</span></label>
                                                        <input required="" class="form-control square" name="city" type="text">
                                                      </div>
                                                      <div class="form-group col-md-6">
                                                        <label>State <span class="required">*</span></label>
                                                        <input required="" class="form-control square" name="state" type="text">
                                                      </div>
                                                      <div class="form-group col-md-6">
                                                        <label>Pincode <span class="required">*</span></label>
                                                        <input required="" type="number" name="pincode" placeholder="Enter Pincode">
                                                      </div>
                                                      <div class="form-group col-md-6">
                                                        <label>Mobile <span class="required">*</span></label>
                                                        <input required="" type="number" name="mobile" placeholder="Enter Mobile number">
                                                      </div>
                                                      <div class="form-group col-md-6">
                                                        <label>Email <span class="required">*</span></label>
                                                        <input type="text" required="" name="email" placeholder="Email">
                                                      </div>
                                                      <div class="col-md-12">
                                                        <button type="submit" class="btn btn-fill-out submit" name="submit" value="Submit">Save</button>
                                                      </div>
                                                    </div>
                                                  </form>
                                            </div>
